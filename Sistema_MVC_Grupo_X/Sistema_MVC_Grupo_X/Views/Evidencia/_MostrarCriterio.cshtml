@model IEnumerable<Sistema_MVC_Grupo_X.Models.Criterio>

@using (Html.BeginForm("Index", "Estudiante", FormMethod.Post, new { id = "frm-Estudiante" }))
{ 
    foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.CheckBox("Modelo",item.modelo_id)
            </td>
            <td>
                @Html.CheckBox("Modelo", item.nombre_spanish)
            </td>
            <td>
                @Html.CheckBox("Modelo", item.nombre_english)
            </td>
            <td>
                @Html.CheckBox("Modelo", item.urlcriterio)
            </td>
            <td>
                @Html.CheckBox("Modelo", item.descripcion)
            </td>
            <td>
                @Html.CheckBox("Modelo", item.estado)
            </td>
        </tr>
    }
}
